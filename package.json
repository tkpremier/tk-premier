{
  "name": "tk-premier",
  "version": "1.0.0",
  "description": "Tommy's side project",
  "main": "dist/app.bundle.js",
  "scripts": {
    "create-drive-table": "node ./db/dev/dbConnection createDriveFilesTable",
    "create-all-tables": "node ./db/dev/dbConnection createAllTables",
    "drop-all-tables": "node ./db/dev/dbConnection dropAllTables",
    "drop-table": "node ./db/dev/dbConnection dropDriveFilesTable",
    "jest-init": "jest --init",
    "test": "jest",
    "babel": "babel app.js -o app.babel.js",
    "build": "yarn webpack",
    "tsc": "tsc",
    "presass": "yarn babel",
    "sass": "node ./views/styles.js ./src/styles.scss",
    "sass-watch": "node ./views/styles.js ./src/styles.scss watch",
    "start": "node dist/server.bundle.js",
    "prewatch": "yarn install",
    "watch": "concurrently \"yarn run webpack-server-watch\" \"yarn run webpack-client-watch\"",
    "serve": "webpack serve --config webpack.server.js --open"
  },
